<?xml version="1.0"?>
<openerp>
    <data>

        <record id="demo_account_journal_credits" model="account.journal">
            <field name="name">Credit</field>
            <field name="code">XCRED</field>
            <field name="debt">True</field>
            <field name="type">cash</field>
            <field name="journal_user">True</field>
        </record>

        <record id="demo_account_journal_credits_fruits"  model="account.journal">
            <field name="name">Credits (Fruits &amp; Vegetables only)</field>
            <field name="code">FCRED</field>
            <field name="debt">True</field>
            <field name="type">cash</field>
            <field name="journal_user">True</field>
            <field name="category_ids" eval="[(6, 0, [ref('point_of_sale.fruits_vegetables')])]"/>
        </record>

        <record id="demo_account_journal_credits_via_discounts"  model="account.journal">
            <field name="name">Discount Credit</field>
            <field name="code">DCRED</field>
            <field name="debt">True</field>
            <field name="type">cash</field>
            <field name="journal_user">True</field>
            <field name="credits_via_discount">True</field>
        </record>

        <!--Standart demo cash method is not created if there are some demo journals already-->
        <record id="demo_account_journal_cash"  model="account.journal">
            <field name="name">Cash</field>
            <field name="code">DECSH</field>
            <field name="debt">False</field>
            <field name="type">cash</field>
            <field name="journal_user">True</field>
        </record>

    </data>
</openerp>
